<Application x:Class="RC_IS.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:RC_IS"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <FontFamily x:Key="Poppins">pack://application:,,,/Resources/Fonts/#Poppins</FontFamily>
        <FontFamily x:Key="PoppinsSymbols">pack://application:,,,/Resources/Fonts/#Poppins Symbols</FontFamily>
    </Application.Resources>
</Application>
